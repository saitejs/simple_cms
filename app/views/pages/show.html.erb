</tr>


<tr>
<th>Position</th>
<td><%= @page.position %></td>
</tr>


<tr>
<th>pemalink</th>
<td><%= @page.permalink %></td>
</tr>

<tr>
<th>subject</th>
<td><%= @page.subject_id %></td>
</tr>


<tr>
<th>Visible</th>
<td><%= @page.visible ? 'true ': 'false' %></td>
</tr>


<tr>
<th>Created</th>
